.sf-menu li li			{ width: 148px; }
.sf-menu li li a		{ background: #FFF; padding: 7px 10px; width: 128px; font-size: 12px; text-transform: none; }
.sf-menu li ul ul		{ margin: -25px 0 0 148px; }
